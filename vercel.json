{
  "version": 2,
  "builds": [
    {
      "src": "src/app/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/fen-tool",
      "dest": "/index.html"
    },
    {
      "src": "/fen-tool/(.*)",
      "dest": "/$1"
    }
  ]}
