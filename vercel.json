{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/fen-tool/(.*)",
      "dest": "/fen-tool/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/fen-tool/api/index"
    },
    {
      "src": "/gen/(.*)",
      "dest": "/fen-tool/gen/index"
    },
    {
      "src": "/(.*)",
      "dest": "/fen-tool/index.html"
    }
  ]
}